{
  "name": "payment-app",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "tsx watch ./src/main.ts",
    "build": "tsc && tsc-alias",
    "debug": "tsc && (concurrently --kill-others \"tsc -w\" \"tsc-alias -w\" \"nodemon --inspect=0.0.0.0:9229 ./dist/apps/payment-app/src/main.js\")",
    "debug:ts": "node --inspect=0.0.0.0:9229 --experimental-specifier-resolution=node --loader ./loader.js ./src/main.ts",
    "lint:check": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/amqplib": "*",
    "@types/express": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-prettier": "*",
    "prettier": "*",
    "ts-node": "*",
    "tsconfig-paths": "*",
    "tsx": "*",
    "typescript": "*",
    "nodemon": "*"
  },
  "dependencies": {
    "amqplib": "^0.10.4"
  }
}
